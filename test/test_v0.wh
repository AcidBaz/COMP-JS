function testIF :
//doit rendre 4
	read X, Y, Z
	%
		nop;
		
		X:=nil;Y:=3;Z:=7;
		X,Y,Z := Y,Z,10;
		
		if X<Z
		then
			X:=X+1
		fi
	%
write X

function testWHILE :
//doit rendre 9	
	read X, Y, Z
	%
		nop;
		
		X:=nil;Y:=3;Z=7;
		X,Y,Z := Y,Z,10;
		
		while X<Z 
		do 
			X:=X+1
		od
write X

function testIFELSE :	
//doit rendre 4
	read X, Y, Z
	%
		nop;
		
		
		
		
		X:=nil;Y:=3;Z=7;
		X,Y,Z := Y,Z,10;
		
		if X>Z then
		X:=0
		else 
		X:=X+1
		fi
write X

function testFORLOOP :
//doit rendre 9
	read X, Y, Z
	%
		nop;
		
		X:=nil;Y:=3;Z=7;
		X,Y,Z := Y,Z,10;
		
		
		for  X<Z
			do
				X:=X+1
		od
		

	% 
write X
