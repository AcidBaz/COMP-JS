function testIF :

	read X, Y, Z
	%
		nop;
		
		X:=nil;Y:=3;Z:=7;
		X,Y,Z := Y,Z,10;
		
		if expression
		then
			X:=Y
		fi
	%
write X

function testWHILE :

	read X, Y, Z
	%
		nop;
		
		X:=nil;Y:=3;Z:=7;
		X,Y,Z := Y,Z,10;
		
		while expression
		do 
			X:=Y
		od
		%
write X

function testIFELSE :	

	read X, Y, Z
	%
		nop;
		
		
		
		
		X:=nil;Y:=3;Z:=7;
		X,Y,Z := Y,Z,10;
		
		if expression then
		X:=0
		else 
		X:=Y
		fi
	%	
write X

function testFORLOOP :

	read X, Y, Z
	%
		nop;
		
		X:=nil;Y:=3;Z:=7;
		X,Y,Z := Y,Z,10;
		
		
		for  expression
			do
				X:=Y
		od
		

	% 
write X
